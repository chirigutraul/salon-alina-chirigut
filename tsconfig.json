{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl":".",
    "paths": {
      "@components":["/components"],
      "@types":["/types"],
      "@icons":["/components/Icons"],
      "@utils":["/utils"],
      "@requests":["/utils/hooks/Requests"]
    },
    "typeRoots": ["/types"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/AppointmentForm/AppointmentCalendar/index.ts"],
  "exclude": ["node_modules"]
}
